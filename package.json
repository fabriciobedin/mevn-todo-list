{
  "name": "mevn-stack",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "SET NODE_ENV=production && babel dev-server --out-dir prod-server && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "concurrently \"SET NODE_ENV=development\" \"babel dev-server --out-dir prod-server --watch\" \"nodemon prod-server/index.js\" \"npm run serve\""
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.5",
    "express": "^4.16.4",
    "jquery": "^3.3.1",
    "moment": "^2.24.0",
    "mongoose": "^5.4.21",
    "popper.js": "^1.14.7",
    "vue": "^2.6.6",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "morgan": "^1.9.1",
    "vue-template-compiler": "^2.5.21"
  }
}
